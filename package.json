{
  "name": "loom",
  "version": "1.0.0",
  "description": "a tool for weaving worlds 1.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec babel-node --inspect server.js",
    "lint": "eslint . --ext .js",
    "lint-fix": "eslint . --ext .js --fix && prettier --write .",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngrayson/loom.git"
  },
  "author": "Nick Grayson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngrayson/loom/issues"
  },
  "homepage": "https://github.com/ngrayson/loom#readme",
  "dependencies": {
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.3",
    "mongoose": "^5.9.2"
  },
  "devDependencies": {
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1"
  },
  "nodemonConfig": {
    "comment": "chatbot/cmds/*.js",
    "ignore": [
      "chatbot/cmds/*.js",
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true
